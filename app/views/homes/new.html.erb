<h1>New Home</h1>

<%= form_tag(homes_url, method: 'post') do %>
  <div>
    <%= label_tag :idcnum %><br />
    <%= number_field_tag :idcnum %>
  </div>

  <div>
    <%= label_tag :name %><br />
    <%= text_field_tag :name %>
  </div>

  <div>
    <%= label_tag :address %><br />
    <%= text_field_tag :address %>
  </div>

  <div>
    <%= label_tag :city %><br />
    <%= text_field_tag :city %>
  </div>

  <div>
    <%= label_tag :state %><br />
    <%= text_field_tag :state %>
  </div>


  <div>
    <%= submit_tag "Create Home" %>
  </div>
<% end %>
